<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.thkong.dnfchatbot.chatbot.dao.ChatbotDao">
	
	<!-- select id는 꼭 Dao의 메소드명과 같아야한다!. -->
	<select id="getSetEquipment" resultType="com.thkong.dnfchatbot.chatbot.vo.Equipment.Equipment" >
		SELECT * 
		FROM   Equipment
		WHERE  setItemName = #{setName}
		ORDER BY printOrder
	</select>
	
</mapper>